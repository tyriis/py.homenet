{# inherits from basic page #}
{% extends '_page.jinja2' %}

{% block title %}Dashboard Home{% endblock %}

{% block usermenu %}
    <div class="usermenu">
        <a class='logout-icon' href="{{ ctx.url('logout') }}"></a>
    </div>
{% endblock %}

{% block content %}
    <div class="list"></div>
{% endblock %}

{% block js %}
    {# google charts api #}
    <script src="https://www.gstatic.com/charts/loader.js"></script>
    {{ js(ctx, 'components/location/charts.js') }}
    {{ js(ctx, 'components/location/details.js') }}
    {{ js(ctx, 'components/location/list.js') }}
    <script>
        require(['components/list']);
    </script>
{% endblock %}
