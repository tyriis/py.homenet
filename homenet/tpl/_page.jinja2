<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>{% block title %}{% endblock %}</title>
    {# load all css elements here #}
    {{ css(ctx) }}
    {# load require first #}
    {{ js(ctx, 'lib/require.js') }}
</head>
<body>
    <header>
        <h1>Dashboard</h1>
        {% block usermenu %}
        {% endblock %}
    </header>

    {% block content %}
    {% endblock %}
    
    {# load all js elements #}
    {% block js %}
        {{ js(ctx, 'lib/ajax.js') }}
        {{ js(ctx) }}
    {% endblock %}
</body>
</html>
